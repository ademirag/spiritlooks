<!DOCTYPE html>
<html lang="en-US">
<head>
  <script type="text/javascript">
  <%#query%>
    var query = "<%query%>";
  <%/query%>
  <%^query%>
    var query = null;
  <%/query%>
  <%#filter%>
    var filter = "<%filter%>";
  <%/filter%>
  <%^filter%>
    var filter = null;
  <%/filter%>
  <%#solo%>
    var solo = "<%solo%>";
  <%/solo%>
  <%^solo%>
    var solo = null;
  <%/solo%>
  <%^query%>
    var query = null;
  <%/query%>
  <%#result%>
    var result = "<%result%>";
  <%/result%>
  <%^result%>
    var result = null;
  <%/result%>
  <%#filterStr%>
    var filterStr = "<%filterStr%>";
  <%/filterStr%>
  <%^filterStr%>
    var filterStr = null;
  <%/filterStr%>

  var appSettings = "<%appSettingsString%>";
  </script>
  <script src="/angular.min.js"></script>
  <script src="/angular-animate.js"></script>
  <script src="/spiritlooks-angular.js"></script>
  <title><%#query%><%query%> - <%/query%>Spirit Looks</title>
  <link rel="stylesheet" type="text/css" href="/spiritlooks.css">

</head>
<body ng-app="spiritlooksApp">

<%#appSettings.ajaxMode%>
  <%> init%>
  <%> main%>
<%/appSettings.ajaxMode%>
<%^appSettings.ajaxMode%>
  <%#query%>
    <%> main%>
  <%/query%>
  <%^query%>
    <%> init%>
  <%/query%>
<%/appSettings.ajaxMode%>
<div id="loading" class="ng-hide"><img src="/load-indicator.gif"></div>
</body>
</html>
